<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FluiFramework.UWP.Core.Styles">
    <!-- LAST UPDATED ON - 17 June 2019 -->
    
    <!-- NOTE: The CornerRadius option has been added in the 1809 SDK, so these conditional parameters are needed to make it properly work under older SDKs -->


    <!-- Fluent Dialog corners -->
    <CornerRadius x:Key="FluentDialogCorners">2</CornerRadius>

    <!-- Fluent ComboBox Corners-->
    <CornerRadius x:Key="FluentComboBoxCorners">2</CornerRadius>

    <!-- Fluent TextBox Corners-->
    <CornerRadius x:Key="FluentTextBoxCorners">2</CornerRadius>

    <!-- Fluent Button Corners -->
    <CornerRadius x:Key="FluentButtonCorners">2</CornerRadius>
    
    
</ResourceDictionary>
